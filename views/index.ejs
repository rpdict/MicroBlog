<%- include header %>
<% posts.forEach(function (post, index) { %>
  <p><h2><a href="u/<%= post.auth %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
  <p class="info">
    <% if (post.name != post.auth) { %>
      由<%= post.name %>转发自<%= post.auth %>
    <% } %>
    作者：<a href="u/<%= post.auth %>"><%= post.auth %></a> |
    日期：<%= post.time.minute %>
  </p>
  <p><%- post.post %></p>
  <% }) %>
<%- include footer %>
